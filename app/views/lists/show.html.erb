<h1>Lists#show</h1>
<h1><%= @board.name %> - <%= @list.name %></h1>
<p><%= @list.body %></p>
<h2> Tasks </h2>
<hr/>
  <% @list.tasks.each do |task| %>
    <%= render "tasks/task", { task: task } %>
  <% end %>
<hr/>
  <%= link_to "Back To Lists", board_lists_path(@board) %>
  <hr/>
  <%= link_to "Add Task", new_list_task_path(@list) %>
  <hr/>
  <%= link_to "Edit List", edit_board_list_path([@board, @list]) %>
  <hr/> 
  <%= link_to "Delete List", [@board, @list], method: :delete %>